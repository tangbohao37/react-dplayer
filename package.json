{
  "name": "react-dplayer",
  "version": "0.4.2",
  "description": "dplayer",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/react-dplayer.min.js",
  "types": "react-dplayer.d.ts",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "sideEffects": false,
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@wii-fe/dplayer": "^1.2.0",
